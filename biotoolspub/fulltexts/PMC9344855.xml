              <th rowspan="1" colspan="1">LRT <italic toggle="yes">P</italic>-value</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td rowspan="1" colspan="1">
                <inline-formula id="IE72">
                  <mml:math id="IM72" display="inline" overflow="scroll">
                    <mml:mrow>
                      <mml:msup>
                        <mml:mrow>
                          <mml:mrow>
                            <mml:mtext>EQMI</mml:mtext>
                          </mml:mrow>
                        </mml:mrow>
