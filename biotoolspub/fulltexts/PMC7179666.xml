<sup><italic>A</italic><sup><italic>p</italic></sup>*</sup>, <sup><italic>A</italic><sup><italic>n</italic></sup>*</sup> using<break/>                                      <sup><italic>A</italic><sup><italic>p</italic></sup>*</sup> = <italic>UR</italic><sup><italic>p</italic></sup><italic>V</italic><sup><italic>T</italic></sup>,<sup><italic>A</italic><sup><italic>n</italic></sup>*</sup> = <italic>UR</italic><sup><italic>n</italic></sup><italic>V</italic><sup><italic>T</italic></sup></td>
