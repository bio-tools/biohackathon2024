                <th rowspan="1" colspan="1">Superpopulation</th>
                <th rowspan="1" colspan="1">PyLAE</th>
                <th rowspan="1" colspan="1">RFMix</th>
              </tr>
            </thead>
            <tbody>
              <tr>
                <td rowspan="1" colspan="1">AFR</td>
