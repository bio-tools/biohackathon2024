                  <mml:mi>N</mml:mi>
                </mml:mrow>
                <mml:mo>+</mml:mo>
                <mml:mrow>
                  <mml:mi>F</mml:mi>
                  <mml:mo>⁢</mml:mo>
                  <mml:mi>P</mml:mi>
                </mml:mrow>
                <mml:mo>+</mml:mo>
                <mml:mrow>
                  <mml:mi>F</mml:mi>
                  <mml:mo>⁢</mml:mo>
                  <mml:mi>N</mml:mi>
                </mml:mrow>
              </mml:mrow>
            </mml:mfrac>
          </mml:mrow>
        </mml:math>
      </disp-formula>
      <disp-formula id="S2.Ex3">
        <mml:math id="M3">
          <mml:mrow>
            <mml:msub>
              <mml:mi>S</mml:mi>
