            </mml:mfrac>
          </mml:mrow>
        </mml:math>
      </disp-formula>
      <disp-formula id="S2.Ex4">
        <mml:math id="M4">
          <mml:mrow>
            <mml:msub>
              <mml:mi>S</mml:mi>
              <mml:mi>n</mml:mi>
