                  </mml:munder>
                </mml:mrow>
                <mml:mrow>
                  <mml:msub>
                    <mml:mi>I</mml:mi>
                    <mml:mi>l</mml:mi>
                  </mml:msub>
                </mml:mrow>
                <mml:mrow>
                  <mml:msub>
                    <mml:mi>I</mml:mi>
                    <mml:mi>k</mml:mi>
                  </mml:msub>
                </mml:mrow>
                <mml:mo>ϕ</mml:mo>
                <mml:mo>(</mml:mo>
              </mml:mrow>
              <mml:mi>m</mml:mi>
              <mml:mrow>
                <mml:msub>
                  <mml:mi>z</mml:mi>
                  <mml:mi>l</mml:mi>
                </mml:msub>
              </mml:mrow>
              <mml:mo>,</mml:mo>
              <mml:mi>m</mml:mi>
              <mml:mrow>
                <mml:msub>
                  <mml:mi>z</mml:mi>
                  <mml:mi>k</mml:mi>
                </mml:msub>
              </mml:mrow>
              <mml:mo>)</mml:mo>
              <mml:mrow>
                <mml:msup>
                  <mml:mo>)</mml:mo>
                  <mml:mn>2</mml:mn>
                </mml:msup>
              </mml:mrow>
