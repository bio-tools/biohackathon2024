              </mml:msub>
            </mml:mrow>
            <mml:mo>ϕ</mml:mo>
            <mml:mo>(</mml:mo>
            <mml:mi>m</mml:mi>
            <mml:mrow>
              <mml:msub>
                <mml:mi>z</mml:mi>
