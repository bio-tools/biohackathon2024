                <mml:mo>(</mml:mo>
                <mml:mrow>
                  <mml:mi>p</mml:mi>
                  <mml:mo>=</mml:mo>
                  <mml:mstyle scriptlevel="0" displaystyle="true">
                    <mml:mrow>
                      <mml:mfrac>
                        <mml:mrow>
