                        </mml:mrow>
                      </mml:mfrac>
                      <mml:munder>
                        <mml:mo>∑</mml:mo>
                        <mml:mrow>
                          <mml:mi>r</mml:mi>
                          <mml:mo>∈</mml:mo>
                          <mml:msub>
