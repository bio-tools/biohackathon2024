				\usepackage{amsfonts} 
				\usepackage{amssymb} 
				\usepackage{amsbsy}
				\usepackage{mathrsfs}
				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$ {M}_p={\sum}_{i=1}^k\sum \limits_{j\in \left\{A,C,G,T\right\}}{f}_{i,j}^p\times {s}_{i,j}^p $$\end{document}</tex-math><mml:math id="M24" display="block"><mml:msub><mml:mi>M</mml:mi><mml:mi>p</mml:mi></mml:msub><mml:mo>=</mml:mo><mml:msubsup><mml:mo>∑</mml:mo><mml:mrow><mml:mi>i</mml:mi><mml:mo>=</mml:mo><mml:mn>1</mml:mn></mml:mrow><mml:mi>k</mml:mi></mml:msubsup><mml:munder><mml:mo>∑</mml:mo><mml:mrow><mml:mi>j</mml:mi><mml:mo>∈</mml:mo><mml:mfenced close="}" open="{" separators=",,,"><mml:mi>A</mml:mi><mml:mi>C</mml:mi><mml:mi>G</mml:mi><mml:mi>T</mml:mi></mml:mfenced></mml:mrow></mml:munder><mml:msubsup><mml:mi>f</mml:mi><mml:mrow><mml:mi>i</mml:mi><mml:mo>,</mml:mo><mml:mi>j</mml:mi></mml:mrow><mml:mi>p</mml:mi></mml:msubsup><mml:mo>×</mml:mo><mml:msubsup><mml:mi>s</mml:mi><mml:mrow><mml:mi>i</mml:mi><mml:mo>,</mml:mo><mml:mi>j</mml:mi></mml:mrow><mml:mi>p</mml:mi></mml:msubsup></mml:math><graphic xlink:href="13059_2019_1847_Article_Equc.gif" position="anchor"/></alternatives></disp-formula>
<disp-formula id="Equd"><alternatives><tex-math id="M25">\documentclass[12pt]{minimal}
				\usepackage{amsmath}
