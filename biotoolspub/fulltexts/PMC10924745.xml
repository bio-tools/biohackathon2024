                    <mml:mrow>
                      <mml:msub>
                        <mml:mover>
                          <mml:mi>U</mml:mi>
                          <mml:mo>Àô</mml:mo>
                        </mml:mover>
                        <mml:mi>I</mml:mi>
                      </mml:msub>
                      <mml:mo>‚Å¢</mml:mo>
                      <mml:mrow>
                        <mml:mo>(</mml:mo>
                        <mml:mi>t</mml:mi>
                        <mml:mo>)</mml:mo>
                      </mml:mrow>
                    </mml:mrow>
                    <mml:mo>=</mml:mo>
                    <mml:mi/>
                  </mml:mrow>
                </mml:mtd>
                <mml:mtd columnalign="left">
                  <mml:mrow>
                    <mml:mrow>
                      <mml:mo>(</mml:mo>
                      <mml:mrow>
                        <mml:mi>Œª</mml:mi>
                        <mml:mo>+</mml:mo>
                        <mml:mi>œà</mml:mi>
                      </mml:mrow>
                      <mml:mo>)</mml:mo>
                    </mml:mrow>
                    <mml:mo>‚Å¢</mml:mo>
                    <mml:msub>
                      <mml:mi>U</mml:mi>
                      <mml:mi>I</mml:mi>
                    </mml:msub>
                    <mml:mo>‚Å¢</mml:mo>
                    <mml:mrow>
                      <mml:mo>(</mml:mo>
                      <mml:mi>t</mml:mi>
