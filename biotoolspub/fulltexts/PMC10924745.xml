                                  <mml:mo>)</mml:mo>
                                </mml:mrow>
                                <mml:msubsup>
                                  <mml:mi>L</mml:mi>
                                  <mml:mi>k</mml:mi>
                                  <mml:mrow>
                                    <mml:mo>(</mml:mo>
                                    <mml:mrow>
                                      <mml:mi>i</mml:mi>
                                      <mml:mo>‚Å¢</mml:mo>
                                      <mml:mn>1</mml:mn>
                                    </mml:mrow>
                                    <mml:mo>)</mml:mo>
                                  </mml:mrow>
                                </mml:msubsup>
                                <mml:mrow>
                                  <mml:mo>(</mml:mo>
                                  <mml:msub>
                                    <mml:mi>t</mml:mi>
