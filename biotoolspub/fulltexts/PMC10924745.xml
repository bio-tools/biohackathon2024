                  <mml:mstyle>
                    <mml:munder>
                      <mml:mo>‚àè</mml:mo>
                      <mml:mrow>
                        <mml:mi>j</mml:mi>
                        <mml:mo>‚àà</mml:mo>
                        <mml:mtext>ùë°ùëñùëùùë†</mml:mtext>
                      </mml:mrow>
                    </mml:munder>
                  </mml:mstyle>
                  <mml:msub>
                    <mml:mi>œà</mml:mi>
                    <mml:mrow>
                      <mml:mi>s</mml:mi>
                      <mml:mo>‚Å¢</mml:mo>
                      <mml:mi>j</mml:mi>
                    </mml:mrow>
                  </mml:msub>
                  <mml:msub>
                    <mml:mi>œÅ</mml:mi>
                    <mml:mrow>
                      <mml:mi>s</mml:mi>
                      <mml:mo>‚Å¢</mml:mo>
                      <mml:mi>j</mml:mi>
                    </mml:mrow>
                  </mml:msub>
                  <mml:mstyle>
                    <mml:munder>
                      <mml:mo>‚àè</mml:mo>
                      <mml:mrow>
                        <mml:mi>i</mml:mi>
                        <mml:mo>‚àà</mml:mo>
                        <mml:mtext mathvariant="italic">intern. nodes</mml:mtext>
                      </mml:mrow>
                    </mml:munder>
                  </mml:mstyle>
                  <mml:mstyle>
                    <mml:munderover>
                      <mml:mo>‚àë</mml:mo>
                      <mml:mrow>
                        <mml:mi>k</mml:mi>
                        <mml:mo>=</mml:mo>
                        <mml:mn>1</mml:mn>
                      </mml:mrow>
                      <mml:mi>d</mml:mi>
                    </mml:munderover>
                  </mml:mstyle>
                  <mml:msub>
                    <mml:mi>Œª</mml:mi>
                    <mml:mrow>
                      <mml:mrow>
                        <mml:mi>s</mml:mi>
                        <mml:mo>‚Å¢</mml:mo>
                        <mml:mi>i</mml:mi>
                      </mml:mrow>
                      <mml:mo>,</mml:mo>
                      <mml:mi>k</mml:mi>
                    </mml:mrow>
                  </mml:msub>
                  <mml:mrow>
                    <mml:mo>(</mml:mo>
                    <mml:msubsup>
                      <mml:mi>p</mml:mi>
                      <mml:mrow>
                        <mml:mrow>
                          <mml:mi>s</mml:mi>
                          <mml:mo>‚Å¢</mml:mo>
                          <mml:mi>i</mml:mi>
                        </mml:mrow>
                        <mml:mo>,</mml:mo>
                        <mml:mrow>
                          <mml:mi>s</mml:mi>
                          <mml:mo>‚Å¢</mml:mo>
                          <mml:mi>i</mml:mi>
                          <mml:mo>‚Å¢</mml:mo>
                          <mml:mn>0</mml:mn>
                        </mml:mrow>
                      </mml:mrow>
                      <mml:mrow>
                        <mml:mo>(</mml:mo>
                        <mml:mrow>
                          <mml:mi>i</mml:mi>
                          <mml:mo>‚Å¢</mml:mo>
                          <mml:mn>0</mml:mn>
                        </mml:mrow>
                        <mml:mo>)</mml:mo>
                      </mml:mrow>
                    </mml:msubsup>
                    <mml:mrow>
                      <mml:mo>(</mml:mo>
                      <mml:msub>
                        <mml:mi>t</mml:mi>
                        <mml:mi>i</mml:mi>
                      </mml:msub>
                      <mml:mo>)</mml:mo>
                    </mml:mrow>
                    <mml:msubsup>
                      <mml:mi>p</mml:mi>
                      <mml:mrow>
                        <mml:mi>k</mml:mi>
                        <mml:mo>,</mml:mo>
                        <mml:mrow>
                          <mml:mi>s</mml:mi>
                          <mml:mo>‚Å¢</mml:mo>
                          <mml:mi>i</mml:mi>
                          <mml:mo>‚Å¢</mml:mo>
                          <mml:mn>1</mml:mn>
                        </mml:mrow>
                      </mml:mrow>
                      <mml:mrow>
                        <mml:mo>(</mml:mo>
                        <mml:mrow>
                          <mml:mi>i</mml:mi>
                          <mml:mo>‚Å¢</mml:mo>
                          <mml:mn>1</mml:mn>
                        </mml:mrow>
                        <mml:mo>)</mml:mo>
                      </mml:mrow>
                    </mml:msubsup>
                    <mml:mrow>
                      <mml:mo>(</mml:mo>
                      <mml:msub>
                        <mml:mi>t</mml:mi>
