                      </mml:mrow>
                      <mml:mn>0</mml:mn>
                    </mml:msubsup>
                    <mml:mo>‚Å¢</mml:mo>
                    <mml:mrow>
                      <mml:mo>(</mml:mo>
                      <mml:mn>0</mml:mn>
                      <mml:mo>)</mml:mo>
                    </mml:mrow>
                    <mml:mo>‚Å¢</mml:mo>
                    <mml:mrow>
                      <mml:mtext mathvariant="italic">¬†</mml:mtext>
                      <mml:mo>‚Üê</mml:mo>
