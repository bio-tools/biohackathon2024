              <mml:mtr>
                <mml:mtd/>
                <mml:mtd columnalign="left">
                  <mml:mrow>
                    <mml:mo>‚àí</mml:mo>
                    <mml:mrow>
                      <mml:mi>œà</mml:mi>
                      <mml:mo>‚Å¢</mml:mo>
                      <mml:mrow>
                        <mml:mo>(</mml:mo>
                        <mml:mrow>
                          <mml:mn>1</mml:mn>
                          <mml:mo>‚àí</mml:mo>
                          <mml:mi>œÅ</mml:mi>
                        </mml:mrow>
                        <mml:mo>)</mml:mo>
                      </mml:mrow>
                      <mml:mo lspace="0.558em">‚Å¢</mml:mo>
