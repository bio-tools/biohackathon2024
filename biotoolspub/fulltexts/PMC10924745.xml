                                  <mml:mi>k</mml:mi>
                                </mml:mrow>
                              </mml:msub>
                              <mml:mrow>
                                <mml:mo>(</mml:mo>
                                <mml:msubsup>
                                  <mml:mi>L</mml:mi>
                                  <mml:mi>s</mml:mi>
                                  <mml:mrow>
                                    <mml:mo>(</mml:mo>
                                    <mml:mrow>
                                      <mml:mi>i</mml:mi>
                                      <mml:mo>‚Å¢</mml:mo>
                                      <mml:mn>0</mml:mn>
                                    </mml:mrow>
                                    <mml:mo>)</mml:mo>
                                  </mml:mrow>
                                </mml:msubsup>
                                <mml:mrow>
