                        </mml:mrow>
                      </mml:msubsup>
                      <mml:mo>‚Å¢</mml:mo>
                      <mml:mrow>
                        <mml:mo>(</mml:mo>
                        <mml:mi>t</mml:mi>
                        <mml:mo>)</mml:mo>
                      </mml:mrow>
                    </mml:mrow>
                    <mml:mo>=</mml:mo>
                    <mml:mi/>
                  </mml:mrow>
                </mml:mtd>
                <mml:mtd columnalign="left">
                  <mml:mrow>
                    <mml:mo>(</mml:mo>
                    <mml:mrow>
                      <mml:mrow>
                        <mml:mstyle>
                          <mml:munderover>
                            <mml:mo>‚àë</mml:mo>
                            <mml:mrow>
                              <mml:mi>k</mml:mi>
                              <mml:mo>=</mml:mo>
                              <mml:mn>1</mml:mn>
                            </mml:mrow>
                            <mml:mi>d</mml:mi>
                          </mml:munderover>
                        </mml:mstyle>
                        <mml:msub>
                          <mml:mi>Œº</mml:mi>
                          <mml:mrow>
                            <mml:mi>s</mml:mi>
                            <mml:mo>‚Å¢</mml:mo>
                            <mml:mi>k</mml:mi>
                          </mml:mrow>
                        </mml:msub>
                      </mml:mrow>
                      <mml:mo>+</mml:mo>
                      <mml:mrow>
                        <mml:mstyle>
                          <mml:munderover>
                            <mml:mo>‚àë</mml:mo>
                            <mml:mrow>
                              <mml:mi>k</mml:mi>
                              <mml:mo>=</mml:mo>
                              <mml:mn>1</mml:mn>
                            </mml:mrow>
                            <mml:mi>d</mml:mi>
                          </mml:munderover>
                        </mml:mstyle>
                        <mml:msub>
                          <mml:mi>Œª</mml:mi>
                          <mml:mrow>
                            <mml:mi>s</mml:mi>
                            <mml:mo>‚Å¢</mml:mo>
