      <disp-formula>
        <mml:math id="m13" display="block" overflow="scroll">
          <mml:mtable>
            <mml:mtr>
              <mml:mtd/>
              <mml:mtd>
                <mml:mrow>
                  <mml:mi/>
                  <mml:mo>=</mml:mo>
                  <mml:mrow>
                    <mml:mrow>
                      <mml:mrow>
                        <mml:msubsup>
                          <mml:mi>p</mml:mi>
                          <mml:mrow>
                            <mml:mi>s</mml:mi>
                            <mml:mo>‚Å¢</mml:mo>
                            <mml:mn>0</mml:mn>
                          </mml:mrow>
                          <mml:mrow>
                            <mml:mo>(</mml:mo>
                            <mml:mn>0</mml:mn>
                            <mml:mo>)</mml:mo>
                          </mml:mrow>
                        </mml:msubsup>
                        <mml:mo>‚Å¢</mml:mo>
                        <mml:mrow>
                          <mml:mo>(</mml:mo>
                          <mml:mn>0</mml:mn>
                          <mml:mo rspace="0.055em">)</mml:mo>
                        </mml:mrow>
                      </mml:mrow>
                      <mml:mo rspace="0.222em">‚ãÖ</mml:mo>
                      <mml:msubsup>
                        <mml:mi>C</mml:mi>
                        <mml:mrow>
                          <mml:mi>s</mml:mi>
