              <mml:mtr>
                <mml:mtd/>
                <mml:mtd columnalign="left">
                  <mml:mrow>
                    <mml:mtext mathvariant="italic">time¬†</mml:mtext>
                    <mml:mrow>
                      <mml:mi mathvariant="normal">Œî</mml:mi>
                      <mml:mo>‚Å¢</mml:mo>
                      <mml:mi>t</mml:mi>
                    </mml:mrow>
                    <mml:mtext mathvariant="italic">¬†</mml:mtext>
                  </mml:mrow>
                </mml:mtd>
              </mml:mtr>
              <mml:mtr>
                <mml:mtd/>
                <mml:mtd columnalign="left">
                  <mml:mrow>
                    <mml:mo>‚àí</mml:mo>
                    <mml:mrow>
                      <mml:mi>Œº</mml:mi>
                      <mml:mo>‚Å¢</mml:mo>
                      <mml:msub>
                        <mml:mi>U</mml:mi>
                        <mml:mi>I</mml:mi>
                      </mml:msub>
                      <mml:mo>‚Å¢</mml:mo>
                      <mml:mrow>
                        <mml:mo>(</mml:mo>
                        <mml:mi>t</mml:mi>
                        <mml:mo>)</mml:mo>
                      </mml:mrow>
                      <mml:mo lspace="0.558em">‚Å¢</mml:mo>
                      <mml:mrow>
                        <mml:mo>‚Üê</mml:mo>
                        <mml:mtext mathvariant="italic">¬†becoming infectious, followed by unsampled</mml:mtext>
