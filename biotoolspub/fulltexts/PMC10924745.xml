                        </mml:mtr>
                      </mml:mtable>
                    </mml:mrow>
                  </mml:mrow>
                </mml:mtd>
              </mml:mtr>
              <mml:mtr>
                <mml:mtd columnalign="left">
                  <mml:mrow>
                    <mml:mrow>
                      <mml:msub>
                        <mml:mover>
                          <mml:mi>U</mml:mi>
                          <mml:mo>Àô</mml:mo>
                        </mml:mover>
                        <mml:mi>s</mml:mi>
                      </mml:msub>
                      <mml:mo>‚Å¢</mml:mo>
                      <mml:mrow>
                        <mml:mo>(</mml:mo>
                        <mml:mi>t</mml:mi>
                        <mml:mo>)</mml:mo>
                      </mml:mrow>
