                                <mml:mtext mathvariant="italic">¬†is a sampled tip in state¬†</mml:mtext>
                                <mml:mi>s</mml:mi>
                              </mml:mrow>
                            </mml:mrow>
                          </mml:mtd>
                          <mml:mtd/>
                        </mml:mtr>
                        <mml:mtr>
                          <mml:mtd columnalign="left">
                            <mml:mrow>
                              <mml:mstyle>
                                <mml:munderover>
                                  <mml:mo>‚àë</mml:mo>
                                  <mml:mrow>
                                    <mml:mi>k</mml:mi>
                                    <mml:mo>=</mml:mo>
                                    <mml:mn>1</mml:mn>
                                  </mml:mrow>
                                  <mml:mi>d</mml:mi>
                                </mml:munderover>
                              </mml:mstyle>
                              <mml:msub>
                                <mml:mi>Œª</mml:mi>
                                <mml:mrow>
                                  <mml:mi>s</mml:mi>
