      <disp-formula id="E9">
        <mml:math id="M9">
          <mml:mtable columnalign="left">
            <mml:mtr>
              <mml:mtd>
                <mml:mtext>// Step 4: Create homomorphism between </mml:mtext>
                <mml:mi>G</mml:mi>
