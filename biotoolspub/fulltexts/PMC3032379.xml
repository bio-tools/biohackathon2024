              </mml:mtd>
            </mml:mtr>
            <mml:mtr>
              <mml:mtd>
                <mml:mtext>Define </mml:mtext>
                <mml:mi>h</mml:mi>
                <mml:mo>:</mml:mo>
                <mml:mi>G</mml:mi>
                <mml:mi>O</mml:mi>
                <mml:mi>T</mml:mi>
                <mml:mtext> in </mml:mtext>
