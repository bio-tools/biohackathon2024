                                      <mml:mo stretchy="false">(</mml:mo>
                                      <mml:mrow>
                                        <mml:mo>¬∑</mml:mo>
                                        <mml:mo>|</mml:mo>
                                        <mml:msub>
                                          <mml:mrow>
                                            <mml:mo>Œª</mml:mo>
                                          </mml:mrow>
                                          <mml:mrow>
