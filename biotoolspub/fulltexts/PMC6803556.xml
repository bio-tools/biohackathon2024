                                <mml:mrow>
                                  <mml:mstyle mathvariant="bold">
                                    <mml:mtext>p</mml:mtext>
                                  </mml:mstyle>
                                </mml:mrow>
                                <mml:mo class="qopname">~</mml:mo>
                              </mml:mover>
                            </mml:mrow>
                            <mml:mrow>
                              <mml:mi>i</mml:mi>
